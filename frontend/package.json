{
	"name": "frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --threshold warning --diagnostic-sources js,svelte",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch --threshold warning --diagnostic-sources js,svelte",
		"lint": "eslint src",
		"lint:fix": "eslint src --fix",
		"format": "prettier --write src",
		"format:check": "prettier --check src"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"autoprefixer": "^10.4.22",
		"eslint": "^9.39.2",
		"eslint-plugin-svelte": "^3.13.1",
		"eslint-plugin-tailwindcss": "^3.18.2",
		"globals": "^16.5.0",
		"postcss": "^8.5.6",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"svelte-eslint-parser": "^1.4.1",
		"tailwindcss": "^3.4.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.51.0",
		"vite": "^7.2.2"
	},
	"dependencies": {
		"heic2any": "^0.0.4",
		"qr-scanner": "^1.4.2",
		"rehype-external-links": "^3.0.0",
		"rehype-sanitize": "^6.0.0",
		"rehype-stringify": "^10.0.1",
		"remark-gfm": "^4.0.1",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.2",
		"unified": "^11.0.5"
	},
	"overrides": {
		"cookie": "^0.7.0"
	}
}
